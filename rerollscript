local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("reroll.gg")

local b = w:CreateFolder("love melynas11")

b:Label("mia khalifa gaming",{
    TextSize = 25;
    TextColor = Color3.fromRGB(255,255,255);
    BgColor = Color3.fromRGB(69,69,69);
    
})


local loopToggle = false
b:Toggle("Auto Open Booster Cases", function(bool)
    loopToggle = bool
    if loopToggle then
        print("Loop started.")
        while loopToggle do
            local ohString1 = "booster_case"
            local ohNumber2 = 3
            game:GetService("ReplicatedStorage").Remotes.OpenCase:InvokeServer(ohString1, ohNumber2)
            wait(1)
        end
    else
        print("Loop stopped.")
    end
end)

